      "cell_type": "markdown",
      "source": [
        "**Preprocess the Data:**"
      ],
      "metadata": {
        "id": "yz8mZacKMqM8"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "from google.colab import drive\n",
        "from sklearn.model_selection import train_test_split\n",
        "import json\n",
        "import pandas as pd\n",
        "import os\n",
        "\n",
        "# Mount Google Drive\n",
        "drive.mount('/content/drive')\n",
        "\n",
        "# Define the path to the dataset in Google Drive\n",
        "data_path = '/content/multiwoz-master/data/MultiWOZ_2.2/schema.json'\n",
        "\n",
        "# Verify if the file exists\n",
        "if not os.path.exists(data_path):\n",
        "    raise FileNotFoundError(\"File '{/content/multiwoz-master/data/MultiWOZ_2.2/schema.json}' not found. Please check the file path.\")\n",
        "\n",
        "# Load the dataset\n",
        "try:\n",
        "    with open(data_path, 'r') as file:\n",
        "        multiwoz_data = json.load(file)\n",
        "except json.JSONDecodeError as e:\n",
        "    print(f\"Error loading JSON: {e}\")\n",
        "    # Optional: Code to handle the error, e.g., loading a fallback file or notifying the user\n",
        "\n",
        "# Assuming 'multiwoz_df' is your DataFrame\n",
        "train_df, test_df = train_test_split(multiwoz_df, test_size=0.2, random_state=42)\n",
        "\n",
        "# Assuming multiwoz_data is a list of dictionaries\n",
        "for dialog in multiwoz_data:\n",
        "   if 'log' in dialog:\n",
        "    for turn in dialog['log']:\n",
        "        # Extract relevant information from each turn\n",
        "        # (e.g., dialog_id, user_utterance, system_utterance, etc.)\n",
        "        # Append to your dialogs list\n",
        "        dialogs.append({\n",
        "            'dialog_id': dialog.get('dialog_id', ''),\n",
        "            'turn_id': turn.get('turn_idx', ''),\n",
        "            'user_utterance': turn.get('usr', ''),\n",
        "            'system_utterance': turn.get('sys', ''),\n",
        "            'dialog_act': turn.get('act', [])\n",
        "        })\n",
        "\n",
        "# Load your data (assuming multiwoz_data contains your data)\n",
        "# Example: multiwoz_data = pd.read_csv('your_data.csv')\n",
        "\n",
        "# Create the DataFrame\n",
        "multiwoz_df = pd.DataFrame(multiwoz_data)\n",
        "\n",
        "# Now you can proceed with train-test split\n",
        "train_df, test_df = train_test_split(multiwoz_df, test_size=0.2, random_state=42)\n",
        "\n",
        "\n",
        "# Split the data into training and testing sets\n",
        "from sklearn.model_selection import train_test_split\n",
        "train_df, test_df = train_test_split(multiwoz_df, test_size=0.2, random_state=42)\n",
        "\n",
        "# Save the training and testing sets to CSV files\n",
        "train_df.to_csv('train.csv', index=False)\n",
        "test_df.to_csv('test.csv', index=False)\n",
        "\n"
      ],
