        "from transformers import BertTokenizer, BertForSequenceClassification\n",
        "from transformers import Trainer, TrainingArguments\n",
        "from sklearn.model_selection import train_test_split\n",
        "import torch\n",
        "import pandas as pd\n",
        "\n",
        "# Load your data correctly\n",
        "train_df = pd.read_csv('/content/train.csv')\n",
        "test_df = pd.read_csv('/content/test.csv')\n",
        "\n",
        "# Now you can proceed with your analysis\n",
        "print(\"Train DataFrame columns:\", train_df.columns)\n",
        "print(train_df.head())\n",
        "\n",
        "print(\"Test DataFrame columns:\", test_df.columns)\n",
        "print(test_df.head())\n",
        "\n",
        "# Assuming 'description' is the correct column for text data\n",
        "text_column = 'description'  # Change this if the column name is different\n",
        "label_column = 'service_name'  # Change this if the column name is different\n",
        "\n",
        "# Add this line to create the numeric label\n",
        "train_df['text'] = train_df[text_column]\n",
        "test_df['text'] = test_df[text_column]\n",
        "\n",
        "# Create numeric labels from 'service_name'\n",
        "label_mapping = {label: idx for idx, label in enumerate(train_df[label_column].unique())}\n",
        "train_df['label'] = train_df[label_column].map(label_mapping)\n",
        "test_df['label'] = test_df[label_column].map(label_mapping)\n",
        "\n",
        "# Verify that 'text' and 'label' columns exist in the DataFrame\n",
        "if 'text' not in train_df.columns:\n",
        "    raise KeyError(\"Column 'text' not found in the DataFrame.\")\n",
        "if 'label' not in train_df.columns:\n",
        "    raise KeyError(\"Column 'label' not found in the DataFrame.\")\n",
        "if 'text' not in test_df.columns:\n",
        "    raise KeyError(\"Column 'text' not found in the Test DataFrame.\")\n",
        "if 'label' not in test_df.columns:\n",
        "    raise KeyError(\"Column 'label' not found in the Test DataFrame.\")\n",
        "\n",
        "# Load the pre-trained BERT tokenizer and model\n",
        "tokenizer = BertTokenizer.from_pretrained('bert-base-uncased')\n",
        "model = BertForSequenceClassification.from_pretrained('bert-base-uncased', num_labels=len(label_mapping))\n",
        "\n",
        "# Tokenize the input data\n",
        "def tokenize_data(df):\n",
        "    return tokenizer(df['text'].tolist(), padding=True, truncation=True, return_tensors=\"pt\")\n",
        "\n",
        "train_encodings = tokenize_data(train_df)\n",
        "test_encodings = tokenize_data(test_df)\n",
        "\n",
        "# Create PyTorch datasets\n",
        "class DialogDataset(torch.utils.data.Dataset):\n",
        "    def __init__(self, encodings, labels):\n",
        "        self.encodings = encodings\n",
        "        self.labels = labels\n",
        "\n",
        "    def __getitem__(self, idx):\n",
        "        item = {key: torch.tensor(val[idx]) for key, val in self.encodings.items()}\n",
        "        item['labels'] = torch.tensor(self.labels[idx])\n",
        "        return item\n",
        "\n",
        "    def __len__(self):\n",
        "        return len(self.labels)\n",
        "\n",
        "train_dataset = DialogDataset(train_encodings, train_df['label'].tolist())\n",
        "test_dataset = DialogDataset(test_encodings, test_df['label'].tolist())\n",
        "\n",
        "# Training arguments\n",
        "training_args = TrainingArguments(\n",
        "    output_dir='./results',          # output directory\n",
        "    num_train_epochs=3,              # total number of training epochs\n",
        "    per_device_train_batch_size=8,   # batch size for training\n",
        "    per_device_eval_batch_size=8,    # batch size for evaluation\n",
        "    warmup_steps=500,                # number of warmup steps for learning rate scheduler\n",
        "    weight_decay=0.01,               # strength of weight decay\n",
        "    logging_dir='./logs',            # directory for storing logs\n",
        "    logging_steps=10,\n",
        ")\n",
        "\n",
        "# Create Trainer instance\n",
        "trainer = Trainer(\n",
        "    model=model,                         # the instantiated 🤗 Transformers model to be trained\n",
        "    args=training_args,                  # training arguments, defined above\n",
        "    train_dataset=train_dataset,         # training dataset\n",
        "    eval_dataset=test_dataset            # evaluation dataset\n",
        ")\n",
        "\n",
        "# Train the model\n",
        "trainer.train()\n"
      ],
      "metadata": {
